{
  "name": "config-pilot",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main/main.js",
  "scripts": {
    "build1": "npm run clean && npm run build:main && npm run build:preload && npm run build:react",
    "build:main": "tsc -p src/main/tsconfig.json",
    "build:preload": "tsc -p src/preload/tsconfig.json",
    "build:react": "vite build",
    "clean": "rimraf dist-electron dist-react node_modules/.cache",
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "test:watch": "vitest watch tests/unit",
    "transpile:electron": "tsc --project src/main/tsconfig.json",
    "dist:mac": "npm run transpile:electron && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run transpile:electron && npm run build && electron-builder --win --x64",
    "dist:linux": "npm run transpile:electron && npm run build && electron-builder --linux --x64"
  },
  "dependencies": {
